{
  "version": "3.3.0",
  "name": "cordova-custom-config-example",
  "cordova_name": "cordova-custom-config-example",
  "description": "Example project to illustrate/validate usage of cordova-custom-config Cordova/Phonegap plugin",
  "author": "Dave Alden",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dpa99c/cordova-custom-config-example.git"
  },
  "cordova": {
